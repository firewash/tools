{
  "globals": {
    "Vue": true
  },
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    // "react",
     "html"
  ],
  "extends": [
    "airbnb"
  ],
  "rules": {
    "comma-dangle": [2, "never"],// 不允许逗号结尾
    "indent": ["warn", 4]
  }
}
